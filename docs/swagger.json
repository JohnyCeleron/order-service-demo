                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/order-service_internal_controllers_response.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/order-service_internal_controllers_response.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
