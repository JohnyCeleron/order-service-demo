{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/order/{order_uid}": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "order"
                ],
                "summary": "Получить заказ по UID",
                "parameters": [
                    {
                        "type": "string",
                        "example": "b563feb7b2b84b6test",
                        "description": "UID заказа",
                        "name": "order_uid",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
